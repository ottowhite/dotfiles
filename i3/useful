# my script for backing up everything from CTT video:
# https://www.youtube.com/watch?v=OEfboN-Nb2s
sudo rsync -aAX --info=progress2 --delete --dry-run --exclude "/dev/*" --exclude "/proc/*" --exclude "/sys/*" --exclude "/tmp/*" --exclude "/run/*" --exclude "/mnt/*" --exclude "/media/*" --exclude "swapfile" --exclude "lost+found" --exclude ".cache" --exclude "Downloads" / /mnt/backup

